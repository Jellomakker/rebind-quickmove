  "authors": [
    "Jellomakker"
  ],
  "contact": {
    "sources": "https://github.com/Jellomakker/aimbotidk"
  },
  "license": "MIT",
  "icon": "assets/quickswaprebind/icon.png",
  "environment": "client",
  "entrypoints": {
